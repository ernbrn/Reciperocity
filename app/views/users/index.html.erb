<h1>Listing Cool People and/or Dogs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  Hay

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><% user.recipes.each do |rec| %></td>
            <td><%= rec.dish_name %></td>
              <% end %>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
