<h1>Listing Cool People and/or Dogs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  Hay

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Sign out forever', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Sign out for now', destroy_user_session_path, :method => :delete %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

